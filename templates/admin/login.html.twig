<div class="container login-container">
    <div class="row">
        <div class="col-md login-form-1">
            <h3>SymfonyShop Dashboard login</h3>
            <form method="post" >
                {% if error %}
                    <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                {% endif %}
                <div class="form-group">
                    <input type="text" name="email" value="admin1@example.com" class="form-control" placeholder="Your Email *"  />
                </div>
                <div class="form-group">
                    <input type="password" name="password" value="admin" class="form-control" placeholder="Your Password *"  />
                </div>
                <div class="form-group">
                    <input type="submit" class="btnSubmit" value="Login" />
                </div>
            </form>
        </div>
    </div>
</div>